
public class tree {
	
	node head;
	
	public tree(){
		head = new node();
	}
	
	
	public void add(int data){
		if (head.data == 0){
			head.data = data;
		}
		
		else{
			node initial = head;
			while(head != null){
				
				if (data == head.data){
					break;
				}
				if (data < head.data){
					head = head.left;
				}
				else{
					head = head.right;
				}
				
			}
			
			if(head == null){
				head = new node();
				head.data = data;
				head = initial;
				System.out.println(""+head.data);
			}

		}
	}
	
	public String getTree(node h){
		node cursor = h;
		if (cursor == null){
			return "";
		}
		else{
			return "" + cursor.data + getTree(cursor.left)+getTree(cursor.right);
		}
	}
}
