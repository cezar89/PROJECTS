
public abstract class Point {
	protected int x;
	protected int y;
	
	public Point(){
		this.x = 0;
		this.y = 0;
	}
	public Point(int x, int y){
		this.x = x;
		this.y = y;
	}

	public int getx(){
		return this.x;
	}
	public int gety(){
		return this.y;
	}
	public void setx(int x){
		this.x = x;
	}
	public void sety(int y){
		this.y = y;
	}
	public void set(int x, int y){
		this.x = x;
		this.y = y;
	}
}
