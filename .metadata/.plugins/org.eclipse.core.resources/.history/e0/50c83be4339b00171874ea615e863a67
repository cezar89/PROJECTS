package QUESTION_2;

public class multDim {

	public static void main(String[] args) {
	    
		int[] initial = {2,5,6,7};
	    int[] list = {2,5,6,7};
	    
	    System.out.print(isZero(list) +"");
    
  
	  
	}
		
	public static boolean isZero(int[] list){
		int sumZero=0;
		for(int i = 0 ; i< list.length ; i++){
			sumZero += list[i];
		}
		System.out.print(sumZero +"");
		if (sumZero == 0){
			return true;
		}
		return false;
	}
	  
}
	  