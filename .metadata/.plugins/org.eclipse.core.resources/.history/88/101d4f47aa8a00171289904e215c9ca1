
public class CompareToMax {

	public static void main(String[] args) {
		int a[] = new int[5];
		a[0]=10;
		a[1]=15;
		a[2]=11;
		a[3]=9;
		a[4]=14;
		
		int n = 5;
		
		int max = compareMax(a,n);
		System.out.printf("The max is: %d",max);
	}

	static int compareMax(int a[],int n){
		int max = 1;
		System.out.printf("Sent array is: %d,%d,%d,%d,%d",a[0],a[1],a[2],a[3],a[4]);
		System.out.printf("Length of the array is: %d",n);
		return max;
	}
}
