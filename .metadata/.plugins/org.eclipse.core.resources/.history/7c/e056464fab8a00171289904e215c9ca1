
public class CompareToMax {

	public static void main(String[] args) {
		int a[] = new int[5];
		a[0]=10;
		a[1]=15;
		a[2]=11;
		a[3]=9;
		a[4]=14;
		
		int n = 5;
		
		int max = compareMax(a,n);
		System.out.printf("The max is: %d\n",max);
	}

	static int compareMax(int a[],int n){
		int max = 0;
		
		for(int i=0;i==4;i++){
			if(a[i]>max){
				max = a[i];
			}
			else{
				continue;
			}
		}

		
		return max;
	}
}
