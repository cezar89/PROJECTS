
public class test {

	public static void main(String[] args) {
		LinkedList l = new LinkedList();
		l.addToEnd(1);
		l.printList();
		System.out.print("\n");
		l.addToEnd(2);
		l.printList();
		System.out.print("\n");		
		l.addToEnd(3);
		l.printList();
		System.out.print("\n");		
		
		l.addToFront(4);
		l.printList();
		
		l.addToFront(5);
		l.printList();
	}

}
