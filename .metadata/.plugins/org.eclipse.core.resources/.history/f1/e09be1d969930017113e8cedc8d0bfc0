
public class test {

	public static void main(String[] args) {
		LinkedList l = new LinkedList();
		l.addToEnd(1);
		l.addToEnd(2);		
		l.addToEnd(3);
		l.addToFront(4);
		l.addToFront(5);
		l.addToEnd(8);
		l.printList();
		System.out.print("\n");
		
		System.out.print("elem: " +l.find(3));
	}

}
