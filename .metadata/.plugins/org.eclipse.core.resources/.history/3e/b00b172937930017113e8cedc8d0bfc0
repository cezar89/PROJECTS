
public class LinkedList {
	int data = 0;
	LinkedList next;
	public LinkedList(){
	}
	
	public void add(int data){
		if (this.data == 0){
			this.data = data;
		}
		else {
			System.out.println("You already have data in the front!");
		}
	}
	
	public String toString(){
		return this.data + "";
	}
}
