
public class tree {
	private node head;
	
	public tree(){
		
	}
	
	public void addInteger(int n){
		if (head == null){
			head.setData(n);
		}
		else{
			node temp = head;
			while (head != null){
				if (head.getData() < n){
					head = head.getRight();
				}
				else{
					head = head.getLeft();
				}
			}
			head.setData(n);
			head = temp;
			
		}
	}
}
