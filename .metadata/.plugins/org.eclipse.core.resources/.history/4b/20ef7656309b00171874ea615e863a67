package QUESTION_2;

public class multDim {

	public static void main(String[] args) {
	    
		int[] initial = {2,5,6,7};
	    int[] list = {2,5,6,7};
	    checkZero(list);
	  
    
  
	  
	}
		
	public static void checkZero(int[] list){
		for(int i = list.length ; i>=0 ; i--){
	    	System.out.println(list[i] +"");
		}
    
	}
	  
}
	  